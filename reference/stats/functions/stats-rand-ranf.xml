<refentry xml:id="function.stats-rand-ranf" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>stats_rand_ranf</refname>
  <refpurpose>Genera un número flotante aleatorio entre 0 y 1</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>float</type><methodname>stats_rand_ranf</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Devuelve un número flotante aleatorio de una distribución uniforme entre 0 (exclusivo) y 1 (exclusivo).
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Un número flotante aleatorio
  </para>
 </refsect1>

</refentry>

